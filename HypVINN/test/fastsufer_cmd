docker run --gpus all -v /groups/ag-reuter/projects/hypothalamus/vinn_version/hypvinn_testsuite/RS/scans:/data \
                      -v /groups/ag-reuter/projects/hypothalamus/vinn_version/hypvinn_testsuite/RS/output:/output \
                      -v /groups/ag-reuter/software-centos/:/fs_license \
                      --entrypoint "/fastsurfer/brun_fastsurfer.sh" \
                      --rm --user $(id -u):$(id -g) deepmi/fastsurfer:latest \
                      --fs_license /fs_license/.license \
                      --sd /output --subject_list /data/subject_list.txt \
                      --parallel --3T

